<mvc:View
		controllerName="project1.controller.DynamicPageListReport"
		height="100%"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:core="sap.ui.core"
		xmlns="sap.m"
		xmlns:f="sap.f"
		xmlns:fb="sap.ui.comp.filterbar"
		xmlns:svm="sap.ui.comp.smartvariants">
			
		<f:DynamicPage headerExpanded="{/headerExpanded}">		
			<f:title>			
				<f:DynamicPageTitle>				
					<f:heading>					
						<svm:SmartVariantManagement id="svm" showExecuteOnSelection="true" />
					</f:heading>					
					<f:expandedContent>				
						<Label id="expandedLabel" text="No filters active" />						
					</f:expandedContent>
					<f:snappedContent>
						<Label id="snappedLabel" text="No filters active" />
					</f:snappedContent>					
				</f:DynamicPageTitle>
			</f:title>
			<f:header>
				<f:DynamicPageHeader>
					<f:content>
						<fb:FilterBar id="filterbar" persistencyKey="myPersKey" useToolbar="false" search=".onSearch" filterChange=".onFilterChange" afterVariantLoad=".onAfterVariantLoad">
							<fb:filterGroupItems>
								<fb:FilterGroupItem name="Name" label="Student Name" groupName="Group1" visibleInFilterBar="true">
									<fb:control>
									
										<MultiComboBox
											name="Name"
											selectionChange=".onSelectionChange"
											items="{
												path: '/NameCategory',
												templateShareable: true
											}">
											<core:Item key="{key}" text="{name}"/>
										</MultiComboBox>
									</fb:control>
								</fb:FilterGroupItem>
								<fb:FilterGroupItem name="Sex" label="Gender" groupName="Group1" visibleInFilterBar="true">
									<fb:control>
										<MultiComboBox
											name="Sex"
											selectionChange=".onSelectionChange"
											items="{
												path: '/SexCategories',
												templateShareable: true
											}">
											<core:Item key="{key}" text="{name}"/>
										</MultiComboBox>
									</fb:control>
								</fb:FilterGroupItem>
								<fb:FilterGroupItem name="App_type" label="App Type" groupName="Group1" visibleInFilterBar="true">
									<fb:control>
										<MultiComboBox
											name="App_type"
											selectionChange=".onSelectionChange"
											items="{
												path: '/App_Category',
												templateShareable: true
											}">
											<core:Item key="{key}" text="{name}"/>
										</MultiComboBox>
									</fb:control>
								</fb:FilterGroupItem>
                                <fb:FilterGroupItem name="MemberDate" label="Member Date" groupName="Group1" visibleInFilterBar="true">
									<fb:control>
										<MultiComboBox
											name="MemberDate"
											selectionChange=".onSelectionChange"
											items="{
												path: '/dateCategories',
												templateShareable: true
											}">
											<core:Item key="{key}" text="{name}"/>
										</MultiComboBox>
									</fb:control>
								</fb:FilterGroupItem>
                                   <fb:FilterGroupItem name="country" label="Country" groupName="Group1" visibleInFilterBar="true">
									<fb:control>
										<MultiComboBox
											name="country"
											selectionChange=".onSelectionChange"
											items="{
												path: '/countryCategories',
												templateShareable: true
											}">
											<core:Item key="{key}" text="{name}"/>
										</MultiComboBox>
									</fb:control>
								</fb:FilterGroupItem>
                                   <fb:FilterGroupItem name="office" label="Office" groupName="Group1" visibleInFilterBar="true">
									<fb:control>
										<MultiComboBox
											name="office"
											selectionChange=".onSelectionChange"
											items="{
												path: '/officeCategories',
												templateShareable: true
											}">
											<core:Item key="{key}" text="{name}"/>
										</MultiComboBox>
									</fb:control>
								</fb:FilterGroupItem>
							</fb:filterGroupItems>
						</fb:FilterBar>
					</f:content>
						<Button  text="copy" press="onCopyTextToClipboard"></Button>
				</f:DynamicPageHeader>				
			</f:header>
			<f:content>
				<Table
					id="table"
					inset="false"
					items="{
						path: '/Userrecord'
					}"
					
				>
					<columns>
                    	<Column minScreenWidth="Tablet" width="10rem" demandPopin="true">
							<Text text="Student DB Id"/>
						</Column>
						<Column minScreenWidth="Tablet" width="10rem" demandPopin="true">
							<Text text="Student Name"/>
						</Column>
						<Column minScreenWidth="Tablet" width="8rem" demandPopin="true">
							<Text text="Gender"/>
						</Column>
						<Column minScreenWidth="Tablet" width="10rem" demandPopin="true">
							<Text text="App Type"/>
						</Column>
                         <Column minScreenWidth="Tablet" width="10rem" demandPopin="true">
							<Text text="Member Date"/>
						</Column>
                        <Column minScreenWidth="Tablet" width="10rem" demandPopin="true">
							<Text text="Country"/>
						</Column>
                        <Column minScreenWidth="Tablet" width="8rem" demandPopin="true">
							<Text text="Office"/>
						</Column>
						   <Column minScreenWidth="Tablet" width="10rem" demandPopin="true">
							<Text text="Count"/>
						</Column>
                        <Column minScreenWidth="Tablet" demandPopin="true">
							<Text text="Status"/>
						</Column>                        
					</columns>
					<items >
						<ColumnListItem  vAlign="Middle" >
							<cells  >
                             <Link text="{StudentDBID}" press="onUserIdClick" />
                              <!-- <Text text="{StudentDBID}" /> -->
								<ObjectIdentifier  title="{Name}" />
								<Text text="{Sex}" />
								<Text text="{App_type}" />
                                <Text text="{MemberDate}" />
                                 <Text text="{country}" />
                                  <Text text="{office}" />   
								 <Text text="{Count}" />   
									<HBox class="vBoxContainer" alignContent="Stretch">  

										<!-- <Button text="CS" press="onpress_cs" type="{=(${CS} >= 1 ? 'Accept' : 'Default')}" />
										<Button text="AP" press="onpress_cs" type="{=(${AP} >= 1 ? 'Accept' : 'Default')}" />
										<Button text="DC" press="onpress_cs" type="{=(${DC} >= 1 ? 'Accept' : 'Default')}" />
										<Button text="FC" press="onpress_cs" type="{=(${FC} >= 1 ? 'Accept' : 'Default')}" />
										<Button text="LF" press="onpress_cs" type="{=(${LF} >= 1 ? 'Reject' : 'Default')}" /> -->


         				    <VBox alignItems="Center"  class="iconContainer">
								<core:Icon press="onpress_cs"  visible="true" hoverBackgroundColor="{= ${CS} === 1 ? '#005f28' : '#000000'}"   backgroundColor="{= ${CS} === 1 ? '#28a745' : '#6c757d'}"  class="iconSize"  /> <!-- Apply icon size class -->
								<VBox class="textOverlay">
								  <Text text="CS" class="textcolor" tooltip="CS"/>
							   </VBox>
							</VBox>
							<VBox alignItems="Center" class="iconContainer">
								<core:Icon press="onpress_AP"  hoverBackgroundColor="{= ${AP} === 1 ? '#005f28' : '#000000'}"  backgroundColor="{= ${AP} === 1 ? '#28a745' : '#6c757d'}" class="iconSize" /> <!-- Apply icon size class -->
								<VBox class="textOverlay" >
								   <Text text="AP"  class="textcolor" tooltip="AP"/>
								</VBox>
							</VBox>
							<VBox alignItems="Center" class="iconContainer">
								<core:Icon press="onpress_DC"   hoverBackgroundColor="{= ${DC} === 1 ? '#005f28' : '#000000'}" backgroundColor="{= ${DC} === 1 ? '#28a745' : '#6c757d'}" class="iconSize"  /> <!-- Apply icon size class -->
								<VBox class="textOverlay">
								  <Text text="DC" class="textcolor" tooltip="DC"/>
							   </VBox>
							</VBox>
							<VBox alignItems="Center" class="iconContainer">
								<core:Icon press="onpress_FC"  hoverBackgroundColor="{= ${FC} === 1 ? '#005f28' : '#000000'}" backgroundColor="{= ${FC} === 1 ? '#28a745' : '#6c757d'}"   class="iconSize" /> <!-- Apply icon size class -->
								<VBox class="textOverlay" >
								  <Text text="FC" class="textcolor" tooltip="FC"/>
							    </VBox>
						    </VBox>
							<VBox alignItems="Center" class="iconContainer">
								<core:Icon   press="onpress_LF"   hoverBackgroundColor="{= ${LF} === 1 ? '#701F28' : '#000000'}"  backgroundColor="{= ${LF} === 1 ? '#dc3545' : '#6c757d'}" class="iconSize"  /> <!-- Apply icon size class -->
								<VBox class="textOverlay">
								   <Text text="LF" class="textcolor" tooltip="LF" />
							    </VBox>
							</VBox>							 
							</HBox>
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</f:content>
		</f:DynamicPage>
</mvc:View>