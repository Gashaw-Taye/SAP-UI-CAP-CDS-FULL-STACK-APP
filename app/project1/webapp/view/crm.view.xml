 <mvc:View
	controllerName="project1.controller.Dash"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:grid="sap.ui.layout.cssgrid">
	<VBox alignItems="Center"  class="iconContainer">
						<core:Icon press="onpress_CS"  visible="true" hoverBackgroundColor="{= ${CS} === 1 ? '#005f28' : '#000000'}"   backgroundColor="{= ${CS} === 1 ? '#28a745' : '#6c757d'}"  class="iconSize"  /> <!-- Apply icon size class -->
						<VBox class="textOverlay">
							<Text text="CS" class="textcolor" tooltip="CS"/>
						</VBox>
							</VBox>
							<VBox alignItems="Center" class="iconContainer">
								<core:Icon press="onpress_AP"  hoverBackgroundColor="{= ${AP} === 1 ? '#005f28' : '#000000'}"  backgroundColor="{= ${AP} === 1 ? '#28a745' : '#6c757d'}" class="iconSize" /> <!-- Apply icon size class -->
								<VBox class="textOverlay" >
								   <Text text="AP"  class="textcolor" tooltip="AP"/>
								</VBox>
							</VBox>
							<VBox alignItems="Center" class="iconContainer">
								<core:Icon press="onpress_DC"   hoverBackgroundColor="{= ${DC} === 1 ? '#005f28' : '#000000'}" backgroundColor="{= ${DC} === 1 ? '#28a745' : '#6c757d'}" class="iconSize"  /> <!-- Apply icon size class -->
								<VBox class="textOverlay">
								  <Text text="DC" class="textcolor" tooltip="DC"/>
							   </VBox>
							</VBox>
							<VBox alignItems="Center" class="iconContainer">
								<core:Icon press="onpress_FC"  hoverBackgroundColor="{= ${FC} === 1 ? '#005f28' : '#000000'}" backgroundColor="{= ${FC} === 1 ? '#28a745' : '#6c757d'}"   class="iconSize" /> <!-- Apply icon size class -->
								<VBox class="textOverlay" >
								  <Text text="FC" class="textcolor" tooltip="FC"/>
							    </VBox>
						    </VBox>
							<VBox alignItems="Center" class="iconContainer">
								<core:Icon   press="onpress_LF"   hoverBackgroundColor="{= ${LF} === 1 ? '#701F28' : '#000000'}"  backgroundColor="{= ${LF} === 1 ? '#dc3545' : '#6c757d'}" class="iconSize"  /> <!-- Apply icon size class -->
								<VBox class="textOverlay">
								   <Text text="LF" class="textcolor" tooltip="LF" />
							    </VBox>
							</VBox>			
							</mvc:View>		