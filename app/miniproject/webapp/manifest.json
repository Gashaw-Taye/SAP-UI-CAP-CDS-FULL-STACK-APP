{
  "_version": "1.58.0",
  "sap.app": {
    "id": "miniproject",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.3",
      "toolsId": "10aa73b5-cc9c-422f-9e41-20adc84c3ff7"
    },
    "dataSources": {
      "mainService": {
        "uri": "/StudentServices/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "mainService_7": {
        "uri": "/StudentServices/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "mainService_1": {
        "uri": "/odata/v4/users/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "mainService_2": {
        "uri": "/odata/v4/schools/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "mainService_3": {
        "uri": "/odata/v4/School_courses/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "mainService_4": {
        "uri": "/odata/v4/Student_communications/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "mainService_5": {
        "uri": "/odata/v4/Student_applications/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "mini_project-manage": {
          "semanticObject": "mini_project",
          "action": "manage",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "miniproject.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$auto"
        }
      },

      "Students": {
        "dataSource": "mainService_7",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$auto"
        }
      },

      "comm": {
        "dataSource": "mainService_4",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$auto"
        }
      },
      "Users": {
        "dataSource": "mainService_1", 
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "miniproject.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteLogin",
          "pattern": ":?query:",
          "target": [
            "TargetLogin"
          ]
        },
        {
          "name": "RouteMaster", 
          "pattern": "Dashboard",
          "target": ["TargetMaster"]
        },

        {
          "name": "Targetdash", 
          "pattern": "Masterpage",
          "target": ["Targetdash"]
        },

        {
          "name": "V", 
          "pattern": "V",
          "target": ["TargetV"]
        },
        {
          "name": "RouteComm", 
          "pattern": "Student_communications",
          "target": ["RouteComm"]
        },
        {
					"name": "RouteStudent",
					"pattern": "Student",
          "target": [
            "RouteStudent"
          ]
				},
        {
          "name": "RouteUsers",
          "pattern": "Users",
          "target": [
            "RouteUsers"
          ]
        },
        
        {
          "name": "RouteList",
          "pattern": "List",
          "target": [
            "RouteList"
          ]
        },
        {
          "name": "AdditionalPage",
          "pattern": "AdditionalPage",
          "target": [
            "TargeAdditionalPage"
          ]
        },
        {
          "name": "RouteUser",
          "pattern": "User",
          "target": [
            "RouteUser"
          ]
        }
      ],
      "targets": {
        "TargetLogin": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Login",
          "viewName": "Login"
        },
        "Targetdash": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Masterpage",
          "viewName": "Masterpage"
        },
        "TargeAdditionalPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "AdditionalPage",
          "viewName": "AdditionalPage"
        },
        "TargetMaster": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Dashboard",
          "viewName": "Dashboard"
        },
        "TargetV": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "V",
          "viewName": "V"
        },
        
        
        "RouteStudent": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Student",
          "viewName": "Student"
        },    
        "RouteComm": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Advisor_Student_Communication",
          "viewName": "Advisor_Student_Communication"
        },     
        "RouteUsers": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Users",
          "viewName": "Users"
        }
        ,        
        "RouteList": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "List",
          "viewName": "List"
        }
        ,        
        "RouteUser": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "User",
          "viewName": "User"
        }
      }
    },
    "rootView": {
      "viewName": "miniproject.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
